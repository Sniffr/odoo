<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="eagle_payment_seq" model="ir.sequence">
        <field name="name">Eagle Payment</field>
        <field name="code">eagle.payment</field>
        <field name="prefix">MP-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="payment_batch_seq" model="ir.sequence">
        <field name="name">Payment Batch</field>
        <field name="code">payment.batch</field>
        <field name="prefix">PB-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="funds_request_seq" model="ir.sequence">
        <field name="name">Funds Request</field>
        <field name="code">funds.request</field>
        <field name="prefix">FR-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="account_request_seq" model="ir.sequence">
        <field name="name">Account Request</field>
        <field name="code">account.request</field>
        <field name="prefix">ARQ-</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
    </record>

</odoo>
